insert into mis_article(title,Category_id,content,campus_id,publisher,create_time,author_id) 	values(	'课室教学设备及服务情况调查表'	,	4	,	'
<small>（说明：本调查表用来了解多媒体设备和工作人员工作状况，以便更好地为教学和师生服务，为便于进一步了解具体问题和沟通反馈，敬请填表人留下姓名和联系方式） </small>
<p>上课校区:&nbsp;&nbsp;&nbsp;&nbsp;教学楼:&nbsp;&nbsp;&nbsp;&nbsp;课室:&nbsp;&nbsp;&nbsp;&nbsp;填表时间:&nbsp;&nbsp;年&nbsp;&nbsp;月&nbsp;&nbsp;日 </p>
