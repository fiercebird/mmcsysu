insert into mis_article(title,Category_id,content,campus_id,publisher,create_time,author_id) 	values(	'小梅送旧有感（多媒体可爱的一员）'	,	3	,	
'<p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   &nbsp;</p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">参加了这次送旧活动，我感觉受益很多！</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">一直把多媒体当做我的另一个家，我已融入其中。<br />   喜欢呆在办公室，喜欢它饱含的和谐与温馨，让我永远觉得温暖！今天既是家人团聚又是分离，我当然要去，我要把我无声的祝福送出去！</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">发言环节<span lang="EN-US">,</span>每位将离开的师兄师姐，还有可爱的主任，都把最真诚的祝福留给了我们！<br />   他们把切身经验，心声之言，无余的送给了我们！<br />   我们不舍，我们受益匪浅。<br />   但我们最想说的是：师兄师姐，主任，辉煌的未来在呼唤着你们！我们永远支持你们！加油！</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">游戏环节笑声满堂。<span lang="EN-US">&ldquo;</span>风情万种<span lang="EN-US">&rdquo;&ldquo;</span>想入非非<span lang="EN-US">&rdquo;&ldquo;</span>闻鸡起舞<span lang="EN-US">&rdquo;&ldquo;</span>心猿意马<span lang="EN-US">&rdquo;&ldquo;</span>小鹿乱转<span lang="EN-US">&rdquo;&ldquo;</span>眉飞色舞<span lang="EN-US">&rdquo;&ldquo;</span>欣喜若狂<span lang="EN-US">&rdquo;</span>等词语的肢体演绎，当局者乐，旁观者更乐！</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">师兄师姐帅气漂亮，才华横溢！美妙歌声如仙境佳音。在表演节目环节展现得淋漓尽致。现场气氛活跃，大家都抱着娱乐大众顺便领取奖品的心态踊跃参加。<br />   一幕幕众人为了奖品而奋斗的场景印入我的脑海。组员团结一致，相互紧拥，更有单脚着地还横抱一人的超难动作，坚持到底的决心可嘉啊！</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">合影环节趣味盎然！主任横抱大雄，体形的对比让我有点不敢相信自己的眼睛。<br />   未毕业的都迫不及待地想穿上学士服，和毕业的师兄们留影，脸上洋溢着幸福的微笑。<br />   我也穿上了学士服，提前三年体味一下毕业的感觉，很快乐，很好玩！三年后再穿上学士服，我想我的感情会变得复杂些了。<br />   与帅气可爱的师兄们<span lang="EN-US">.</span>主任合了影。永远的记忆，永远的留恋，值得珍藏一生。</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">发了纸片写祝福语，可是我想说的太多，一时之间却不知从何说起。喜欢用文字来表述自己，喜欢把好的事儿都记入日记里，以前是手写，现在是轻敲。<br />   很多话说不出口，但会用心去静静地倾诉，然后再轻轻地拾起，存在心灵深处。</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal" style="margin-top:0cm;margin-right:0cm;margin-bottom:8.4pt;  margin-left:0cm;mso-pagination:widow-orphan">   <span style="font-size:14px;"><span style="font-family: 宋体; ">有你有我，祝福无处不在！阳光，灿烂，幸福，美好的生活伴随着<br />   每一位多媒体大家庭成员！<br />   <span lang="EN-US">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &mdash;&mdash;</span>小梅</span></span><span style="font-size:12.0pt;  font-family:宋体;mso-bidi-font-family:宋体;mso-font-kerning:0pt"><span lang="EN-US"><o:p></o:p></span></span></p>  <p class="MsoNormal">   <span style="font-size:14px;"><span style="font-family: 宋体; ">（有感而发，一发还不可收拾，见笑啦！）</span></span></p>
'	,	10	,	'东校区多媒体中心'    ,   	'2011-06-07 10:04:11.637'	,	'9'	);
insert into mis_article(title,Category_id,content,campus_id,publisher,create_time,author_id) 	values(	'东校区多媒体活动征文评定'	,	3	,	
'<p style="margin-left:50.25pt;">   &nbsp;</p>  <br />  <div style="text-indent: 28px;">   <br />   <div>    <div style="font-family: Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); ">     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;这次征文中获采用的将会在我们多媒体的网站中展示，在此我谨代表多媒体网站设计小组对投稿的助理表示衷心的感谢！往后还会继续有类似的活动，而且佳鑫有指示：要努力给大家加薪！你们懂的O(&cap;_&cap;)O</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;根据这次征文的情况，在此修改几点征文要求和提一些意见，如下：</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;1.群邮中规定&ldquo;感想不能敷衍了事&rdquo;，&ldquo;相当严格&rdquo;，貌似吓着大家了，搞到这次收集到得稿子不太多，囧&hellip;&hellip;其实这条不会太紧的，大家只要认真写点，不是明显虚情假意、过分凑字数，都会得到应有的褒奖~稿子是用来给我们自己的网站做展示的，多点真情流露，不必太死板；</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;2.通讯稿要注意记录整个活动整个事件，要有全局观，不能像感想那样只集中与一点或一事，有部分通讯稿在这点做得不够好，望改进！想想报纸上通讯稿的写法，都是有始有终的。当然不要求像报纸那样死板，放到网上的东西太死板没人看。个人比较欣赏袁竞逐同学聚餐篇通讯稿的写法（除了有真情实感外，还附了照片，觉得挺不错的）；</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;3.文档命名要按规定，群邮的回复上有详细说明，望遵循；</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;4.不想花时间写通讯的同学，可以写感想，字数虽然说尽量300字以内，但是超了也没关系，重要的是有参与，一经采用，也会再加薪；</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;5.往后应网站建设需要，可能会继续收集各种投稿，届时另行通知。另外网站还在设计与调试中，获采用文章会在适当时间一并发布；</span></p>     <p>      <span style="font-size: 14px; ">&nbsp; &nbsp; &nbsp; &nbsp;6.如对征文奖评情况有异议，或者有更好的建议，请把个人意见写成文档发送到527174922@qq.com</span></p>     <p style="margin-left: 50.25pt; ">      &nbsp;</p>     <p style="margin-left: 50.25pt; ">      &nbsp;</p>    </div>   </div>  </div>  <p>   &nbsp;</p>
'	,	10	,	'东校区多媒体中心'    ,   	'2011-06-07 08:11:14.817'	,	'1'	);
insert into mis_article(title,Category_id,content,campus_id,publisher,create_time,author_id) 	values(	'东校区课室借用设备流程'	,	2	,	
